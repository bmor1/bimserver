<project name="BIMserver">
	<property name="version" value="trunk" />
	<target name="jar">
		<ant antfile="bimserver.build.xml" target="Build Jar"></ant>
	</target>
	<target name="war">
		<ant antfile="bimserver.build.xml" target="Build War"></ant>
	</target>
	<target name="client">
		<ant antfile="client.build.xml" target="Build Client"></ant>
	</target>
	<target name="clientlib">
		<ant antfile="clientlib.build.xml" target="Build ClientLib"></ant>
	</target>
	<target name="combinedjar">
		<ant antfile="combined.build.xml" target="Build CombinedJar"></ant>
	</target>
	<target name="combinedwar">
		<ant antfile="combined.build.xml" target="Build CombinedWar"></ant>
	</target>
	<target name="satellitejar">
		<ant antfile="satellite.build.xml" target="Build Jar"></ant>
	</target>
	<target name="satellitewar">
		<ant antfile="satellite.build.xml" target="Build War"></ant>
	</target>
	<target name="serverlib">
		<ant antfile="bimserverlib.build.xml" target="Build Server Lib"></ant>
